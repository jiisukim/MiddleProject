package vo;

public class CampRevVO {

	private String revCode;
	private String memCode;
	private String bkCode;
	private String revTitle;
	private String revCon;
	private String revDate;
	private String revCmpvst;
	
	public String getRevCode() {
		return revCode;
	}
	public void setRevCode(String revCode) {
		this.revCode = revCode;
	}
	public String getMemCode() {
		return memCode;
	}
	public void setMemCode(String memCode) {
		this.memCode = memCode;
	}
	public String getBkCode() {
		return bkCode;
	}
	public void setBkCode(String bkCode) {
		this.bkCode = bkCode;
	}
	public String getRevTitle() {
		return revTitle;
	}
	public void setRevTitle(String revTitle) {
		this.revTitle = revTitle;
	}
	public String getRevCon() {
		return revCon;
	}
	public void setRevCon(String revCon) {
		this.revCon = revCon;
	}
	public String getRevDate() {
		return revDate;
	}
	public void setRevDate(String revDate) {
		this.revDate = revDate;
	}
	public String getRevCmpvst() {
		return revCmpvst;
	}
	public void setRevCmpvst(String revCmpvst) {
		this.revCmpvst = revCmpvst;
	}
	
	

		
}
