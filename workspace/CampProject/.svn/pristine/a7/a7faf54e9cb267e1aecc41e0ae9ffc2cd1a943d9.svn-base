package vo;

public class RevRplVO {
	
	private String RplCode;
	private String RevCode;
	private String CmpCode;
	private String RplCon;
	private String RplDate;
	
	public String getRplCode() {
		return RplCode;
	}
	public void setRplCode(String rplCode) {
		RplCode = rplCode;
	}
	public String getRevCode() {
		return RevCode;
	}
	public void setRevCode(String revCode) {
		RevCode = revCode;
	}
	public String getCmpCode() {
		return CmpCode;
	}
	public void setCmpCode(String cmpCode) {
		CmpCode = cmpCode;
	}
	public String getRplCon() {
		return RplCon;
	}
	public void setRplCon(String rplCon) {
		RplCon = rplCon;
	}
	public String getRplDate() {
		return RplDate;
	}
	public void setRplDate(String rplDate) {
		RplDate = rplDate;
	}	 
	
	
}
